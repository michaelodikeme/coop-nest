{
  "name": "coop-nest-server",
  "version": "1.0.0",
  "description": "A cooperative savings and loan management application built with TypeScript",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "seed": "ts-node prisma/seed.ts",
    "seed:smart": "ts-node prisma/seed_m.ts",
    "backup:db": "pg_dump --clean --if-exists --no-owner --username=postgres --host=postgres --port=5432 --dbname=postgres --schema=coop-nest_db > backups/db/coop-nest_db_backup.sql",
    "reset:db": "npm run backup:db && npx prisma db reset",
    "restore:db": "psql --username=postgres --host=docker-postgres-server --port=5432 --dbname=postgres < backups/db/coop_nest.sql",
    "migrate:db": "npx prisma migrate dev"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@prisma/client": "^6.10.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/pdfkit": "^0.13.9",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "country-codes-list": "^2.0.0",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.4",
    "fs": "^0.0.1-security",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.7",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "pdfkit": "^0.17.1",
    "rate-limit-redis": "^4.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.5.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@express-rate-limit/tsconfig": "^1.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.0",
    "@types/joi": "^17.2.2",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.0",
    "jest": "^27.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node prisma/seed.ts",
    "backup:db": "pg_dump --clean --if-exists --no-owner --username=postgres --host=localhost --port=5432 --dbname=postgres > backups/db/db_backup.sql"
  }
}
